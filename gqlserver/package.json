{
  "name": "as4-generating-types-example",
  "version": "1.0.0",
  "description": "An example of generating types from a schema to use in your resolvers.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml",
    "compile": "npm run generate && tsc",
    "postinstall": "npm run compile",
    "start": "npm run compile && node ./dist/index.js",
    "watch": "concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\"  \"tsc -w \" \"nodemon ./dist/index.js \""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/datasource-rest": "^6.3.0",
    "@apollo/server": "^4.1.0",
    "@opentelemetry/api": "1.0",
    "@opentelemetry/core": "1.0",
    "@opentelemetry/exporter-zipkin": "1.0",
    "@opentelemetry/instrumentation": "^0.27.0",
    "@opentelemetry/instrumentation-express": "^0.28.0",
    "@opentelemetry/instrumentation-graphql": "^0.27.4",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/resources": "1.0",
    "@opentelemetry/sdk-trace-base": "1.0",
    "@opentelemetry/sdk-trace-node": "1.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.23.0",
    "graphql-tag": "^2.12.6",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/typescript": "2.8.8",
    "@graphql-codegen/typescript-resolvers": "2.7.13",
    "@types/node": "^18.7.18",
    "concurrently": "7.6.0",
    "nodemon": "2.0.22"
  }
}
